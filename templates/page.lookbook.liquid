<div class="row">
<header class="page-header">
  {% if settings.enable_multilang %}
  <h4 class="lang1">{{ page.title | split: '|' | first }}</h4>
  <h4 class="lang2">{{ page.title | split: '|' | last }}</h4>
  {% else %}
  <h4>{{ page.title | split: '|' | first }}</h4>   
  {% endif %}  
</header>

{% comment %}
  Regular page content goes here.
{% endcomment %}
<div class="rte">
  {% section 'lookbook'%}
</div>
</div>