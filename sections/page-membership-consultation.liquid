{% schema %}
  {
    "name": " Membership Consultation",
    "class": "index-section-V2signup index-section--flush",
    "settings": [
        
    {
        "type": "checkbox",
        "id": "display_image_cat",
        "label": "Display Image Category?",
        "default": true
    },
    {
        "type": "image_picker",
        "id": "img_cat",
        "label": "Image Category"
    },
    {
        "type": "checkbox",
        "id": "enable",
        "label": "Enable",
        "default": true
    }, 
    {
        "type": "image_picker",
        "id": "signup_banner",
        "label": "Banner" 
    }, 

    {
        "type": "header",
        "content": { 
        "en": "Membership Form Popup"
        }
    },
    {
        "type": "html",
        "id": "member_popup_title",
        "label": "Member Popup Title",
        "default": "Title"
    },
    {
        "type": "html",
        "id": "member_popup_content",
        "label": "Member Popup Content"
           
    },
    {
        "type": "header",
        "content": { 
        "en": "Membership Heading"
        }
    },
    {
        "type": "html",
        "id": "member_title",
        "label": "Membership Title",
        "default": "Let Us Help You Optimize Your Health."
    },
    {
        "type": "html",
        "id": "member_content",
        "label": "Membership Content"
    },
    {
        "type": "image_picker",
        "id": "table-side-image",
        "label": "Side Image"
    },
    {
        "type": "header",
        "content": { 
        "en": "Membership Table Heading"
        }
    },
    {
        "type": "html",
        "id": "member_table_title",
        "label": "Membership Table Title",
        "default": "What Should I Expect?"
    },
    {
        "type": "html",
        "id": "member_table_content",
        "label": "Membership Table Content"
    }


    ],
    "presets": [
    {
        "name": "Signup V2Section",
        "category": "Advanced layout"
    }
    ]
  }
{% endschema %}
