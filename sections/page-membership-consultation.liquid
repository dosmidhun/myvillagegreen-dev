<div id="member-ship-table">
    <div class="container">
        <div class="row">
            <div class="col-sm-12">
                <h2 id="member-ship-title">{{ section.settings.member_title }}</h2>
                <p id="member-ship-desc">{{ section.settings.member_content }}</p>
            </div>
        </div>
        <div class="row" id="member_conslt_cont_sec">
            <div class="col-sm-6">
                <div class="memberconsult_left">
                    <img id="member-ship-img" src="{{ section.settings.table-side-image | img_url :"master"}}" alt="{{ page.title | split: '|' | first }}" />
                </div>
            </div>
            <div class="col-sm-6">
                <div class="memberconsult_right">
                    <h3 id="member-ship-title-sub">{{ section.settings.member_table_title }}</h3>
                    {{ section.settings.member_table_content }} 
                    <a href="javascript:void(0)" id="member-ship-link" class="unlock_click" title="Schedule now" data-toggle="modal" data-target="#membership_consultation_popup"> Schedule Now </a>
                </div>
            </div>
        </div>
    </div>
</div>

